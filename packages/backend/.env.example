# AWS Credentials
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
AWS_REGION=us-east-1

# S3 Bucket for video storage
S3_BUCKET_NAME=your-video-bucket-name

# DynamoDB Table for video metadata
DYNAMODB_TABLE_NAME=your-video-table-name

# FFmpeg path (if not in system PATH)
FFMPEG_PATH=/path/to/ffmpeg

# Maximum file size for video uploads (in bytes)
MAX_FILE_SIZE=104857600

# Allowed file types for upload (comma-separated)
ALLOWED_FILE_TYPES=video/mp4,video/quicktime

# JWT Secret for authentication (if implemented)
JWT_SECRET=your_jwt_secret

# Optional: Sentry DSN for error tracking
SENTRY_DSN=

